{
  "version": "8",
  "dockerImage": "rqlite/rqlite:8",
  "connectionStringTemplate": "http://localhost:{HOST_PORT}",
  "ports": [{ "containerPort": 4001, "protocol": "tcp" }],
  "volumes": [],
  "defaultEnvironment": {},
  "defaultConfiguration": {},
  "healthcheck": {
    "test": ["CMD-SHELL", "wget -qO- http://localhost:4001/status || exit 1"],
    "interval": "30s",
    "timeout": "10s",
    "retries": 3,
    "startPeriod": "5s"
  }
}
