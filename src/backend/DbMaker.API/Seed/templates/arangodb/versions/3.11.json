{
  "version": "3.11",
  "dockerImage": "arangodb:3.11",
  "connectionStringTemplate": "http://root:{ARANGO_ROOT_PASSWORD}@localhost:{HOST_PORT}",
  "ports": [{ "containerPort": 8529, "protocol": "tcp" }],
  "volumes": [{ "containerPath": "/var/lib/arangodb3" }],
  "defaultEnvironment": {
    "ARANGO_ROOT_PASSWORD": "secure_password_123"
  },
  "defaultConfiguration": {},
  "healthcheck": {
    "test": ["CMD-SHELL", "wget -qO- http://localhost:8529/_api/version || exit 1"],
    "interval": "30s",
    "timeout": "10s",
    "retries": 3,
    "startPeriod": "10s"
  }
}
